{
  "name": "vue-template-crafter",
  "version": "0.0.6",
  "description": "An awesome template wizard",
  "main": "./dist/vue-template-crafter.umd.js",
  "module": "./dist/vue-template-crafter.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-template-crafter.mjs",
      "require": "./dist/vue-template-crafter.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && copyfiles -u 1 src/**/*.d.ts dist",
    "watch": "vite build --watch",
    "dev": "vite",
    "serve": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library",
    "form"
  ],
  "author": "Daniel Grabasch",
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "uuid": "^10.0.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "copyfiles": "^2.4.1",
    "sass": "^1.77.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabbarowski/vue-template-crafter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gabbarowski/vue-template-crafter/issues"
  },
  "homepage": "https://github.com/Gabbarowski/vue-template-crafter#readme"
}
